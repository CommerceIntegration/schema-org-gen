{
	"id": "http://json-schema.org/schemas/AnatomicalStructure.json",
	"title": "Anatomical Structure",
	"format": "http://schema.org/AnatomicalStructure",
	"allOf": [{"$ref": "MedicalEntity.json"}],
	"type": "object",
	"properties": {
		"associatedPathophysiology": {"$ref": "#/definitions/associatedPathophysiology"},
		"bodyLocation": {"$ref": "#/definitions/bodyLocation"},
		"connectedTo": {
			"type": "array",
			"items": {"$ref": "AnatomicalStructure.json#/definitions/possibleRef"}
		},
		"diagram": {
			"type": "array",
			"items": {"$ref": "ImageObject.json#/definitions/possibleRef"}
		},
		"function": {"$ref": "#/definitions/function"},
		"partOfSystem": {"$ref": "AnatomicalSystem.json#/definitions/possibleRef"},
		"relatedCondition": {
			"type": "array",
			"items": {"$ref": "MedicalCondition.json#/definitions/possibleRef"}
		},
		"relatedTherapy": {
			"type": "array",
			"items": {"$ref": "MedicalTherapy.json#/definitions/possibleRef"}
		},
		"subStructure": {
			"type": "array",
			"items": {"$ref": "AnatomicalStructure.json#/definitions/possibleRef"}
		}
	},
	"definitions": {
		"array": {
			"type": "array",
			"items": {"$ref": "#"}
		},
		"possibleRef": {
			"oneOf": [
				{"$ref": "#"},
				{
					"type": "string",
					"format": "uri",
					"links": [{
						"rel": "full",
						"href": "{+$}"
					}]
				}
			]
		},
		"associatedPathophysiology": {
			"title": "Associated Pathophysiology",
			"description": "If applicable, a description of the pathophysiology associated with the anatomical system, including potential abnormal changes in the mechanical, physical, and biochemical functions of the system.",
			"type": "string"
		},
		"bodyLocation": {
			"title": "Body Location",
			"description": "Location in the body of the anatomical structure.",
			"type": "string"
		},
		"function": {
			"title": "Function",
			"description": "Function of the anatomical structure.",
			"type": "string"
		}
	}
