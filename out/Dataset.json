{
	"id": "http://json-schema.org/schemas/Dataset.json",
	"title": "Dataset",
	"format": "http://schema.org/Dataset",
	"allOf": [{"$ref": "CreativeWork.json"}],
	"type": "object",
	"properties": {
		"catalog": {
			"type": "array",
			"items": {"$ref": "DataCatalog.json#/definitions/possibleRef"}
		},
		"distribution": {
			"type": "array",
			"items": {"$ref": "DataDownload.json#/definitions/possibleRef"}
		},
		"spatial": {"$ref": "Place.json#/definitions/possibleRef"},
		"temporal": {"$ref": "#/definitions/temporal"}
	},
	"definitions": {
		"array": {
			"type": "array",
			"items": {"$ref": "#"}
		},
		"possibleRef": {
			"oneOf": [
				{"$ref": "#"},
				{
					"type": "string",
					"format": "uri",
					"links": [{
						"rel": "full",
						"href": "{+$}"
					}]
				}
			]
		},
		"temporal": {
			"title": "Temporal",
			"description": "The range of temporal applicability of a dataset, e.g. for a 2011 census dataset, the year 2011 (in ISO 8601 time interval format).",
			"type": "string",
			"format": "date-time"
		}
	}
}