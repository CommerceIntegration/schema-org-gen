{
	"id": "http://json-schema.org/schemas/MediaObject.json",
	"title": "Media Object",
	"format": "http://schema.org/MediaObject",
	"allOf": [{"$ref": "CreativeWork.json"}],
	"type": "object",
	"properties": {
		"associatedArticle": {
			"type": "array",
			"items": {"$ref": "NewsArticle.json#/definitions/possibleRef"}
		},
		"bitrate": {"$ref": "#/definitions/bitrate"},
		"contentSize": {"$ref": "#/definitions/contentSize"},
		"contentUrl": {"$ref": "#/definitions/contentUrl"},
		"duration": {"$ref": "Duration.json#/definitions/possibleRef"},
		"embedUrl": {
			"type": "array",
			"items": {"$ref": "#/definitions/embedUrl"}
		},
		"encodesCreativeWork": {"$ref": "CreativeWork.json#/definitions/possibleRef"},
		"encodingFormat": {"$ref": "#/definitions/encodingFormat"},
		"expires": {"$ref": "#/definitions/expires"},
		"height": {"$ref": "#/definitions/height"},
		"playerType": {
			"type": "array",
			"items": {"$ref": "#/definitions/playerType"}
		},
		"productionCompany": {"$ref": "Organization.json#/definitions/possibleRef"},
		"publication": {
			"type": "array",
			"items": {"$ref": "PublicationEvent.json#/definitions/possibleRef"}
		},
		"regionsAllowed": {"$ref": "Place.json#/definitions/possibleRef"},
		"requiresSubscription": {"$ref": "#/definitions/requiresSubscription"},
		"uploadDate": {"$ref": "#/definitions/uploadDate"},
		"width": {"$ref": "#/definitions/width"}
	},
	"definitions": {
		"array": {
			"type": "array",
			"items": {"$ref": "#"}
		},
		"bitrate": {
			"title": "Bitrate",
			"description": "The bitrate of the media object.",
			"type": "string"
		},
		"contentSize": {
			"title": "Content Size",
			"description": "File size in (mega/kilo) bytes.",
			"type": "string"
		},
		"contentUrl": {
			"title": "Content Url",
			"description": "Actual bytes of the media object, for example the image file or video file. (previous spelling: contentURL)",
			"type": "string",
			"format": "uri",
			"links": [{
				"rel": "full",
				"href": "{+$}"
			}]
		},
		"embedUrl": {
			"title": "Embed Url",
			"description": "A URL pointing to a player for a specific video. In general, this is the information in the src element of an embed tag and should not be the same as the content of the loc tag. (previous spelling: embedURL)",
			"type": "string",
			"format": "uri",
			"links": [{
				"rel": "full",
				"href": "{+$}"
			}]
		},
		"encodingFormat": {
			"title": "Encoding Format",
			"description": "mp3, mpeg4, etc.",
			"type": "string"
		},
		"expires": {
			"title": "Expires",
			"description": "Date the content expires and is no longer useful or available. Useful for videos.",
			"type": "string",
			"format": "date"
		},
		"height": {
			"title": "Height",
			"description": "The height of the item.",
			"anyOf": [{"$ref": "Distance.json#/definitions/possibleRef"}, {"$ref": "QuantitativeValue.json#/definitions/possibleRef"}]
		},
		"playerType": {
			"title": "Player Type",
			"description": "Player type requiredâ€”for example, Flash or Silverlight.",
			"type": "string"
		},
		"requiresSubscription": {
			"title": "Requires Subscription",
			"description": "Indicates if use of the media require a subscription  (either paid or free). Allowed values are true or false (note that an earlier version had 'yes', 'no').",
			"type": "boolean"
		},
		"uploadDate": {
			"title": "Upload Date",
			"description": "Date when this media object was uploaded to this site.",
			"type": "string",
			"format": "date"
		},
		"width": {
			"title": "Width",
			"description": "The width of the item.",
			"anyOf": [{"$ref": "Distance.json#/definitions/possibleRef"}, {"$ref": "QuantitativeValue.json#/definitions/possibleRef"}]
		}
	}
}