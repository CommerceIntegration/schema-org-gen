{
	"id": "http://json-schema.org/schemas/MusicPlaylist.json",
	"title": "Music Playlist",
	"format": "http://schema.org/MusicPlaylist",
	"allOf": [{"$ref": "CreativeWork.json"}],
	"type": "object",
	"properties": {
		"numTracks": {"$ref": "#/definitions/numTracks"},
		"track": {
			"type": "array",
			"items": {"$ref": "MusicRecording.json#/definitions/possibleRef"}
		}
	},
	"definitions": {
		"array": {
			"type": "array",
			"items": {"$ref": "#"}
		},
		"possibleRef": {
			"oneOf": [
				{"$ref": "#"},
				{
					"type": "string",
					"format": "uri",
					"links": [{
						"rel": "full",
						"href": "{+$}"
					}]
				}
			]
		},
		"numTracks": {
			"title": "Num Tracks",
			"description": "The number of tracks in this album or playlist.",
			"type": "integer"
